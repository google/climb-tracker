<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.steren.climbtracker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ClimbTracker"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ClimbSessionDetailActivity"
            android:label="@string/title_climbsession_detail"
            android:parentActivityName=".ClimbTracker" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ClimbTracker" />
        </activity>
    </application>

</manifest>
